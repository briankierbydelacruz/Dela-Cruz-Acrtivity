body{
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    font-size: 20px;
}

.nav {
    background-color: #333;
    display: flex;
    justify-content: right;
    padding: 10px 10px; 
}

.navbar {
    list-style-type: none;
    margin: 0;
    padding: 0;
    display: flex;
}

.navbar li {
    position: relative;
}

.navbar li a {
    display: block;
    padding: 20px;
    color: white;
    text-decoration: none;
    
}

.navbar li a:hover {
    background-color: #575757;
}

.dropdown {
    position: relative;
}

.dropbtn {
    background-color: #333;
    color: white;
    padding: 15px;
    font-size: 20px;
    border: none;
    cursor: pointer;
}

.drpdwn-content {
    display: none;
    position: absolute;
    background-color: #333;
    min-width: 160px;
    z-index: 1;
}

.drpdwn-content a {
    padding: 12px 16px;
    text-decoration: none;
    display: block;
    color: white;
}

.drpdwn-content a:hover {
    background-color: #575757;
}

.dropdown:hover .drpdwn-content {
    display: block;
}

.iframe-container {
    width: 100%;
    height: 500px;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 20px;
  }

  iframe {
    width: 80%;
    height: 100%;
    border: 2px solid #ddd;
    border-radius: 10px;
  }